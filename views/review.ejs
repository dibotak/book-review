<%- include('./partial/header')%>

<div class="container mt-5">
<form action="/book/review/<%= data.id%>/add" method="POST">
    <div class="form-group">
        <h1><%= data.title%></h1>
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Review : </label>
        <textarea name="review" class="form-control rounded-0" id="exampleFormControlTextarea1" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</body>
